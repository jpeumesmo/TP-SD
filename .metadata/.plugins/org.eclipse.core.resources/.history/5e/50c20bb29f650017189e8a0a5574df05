
public class ServidorSenha {

	ServidorSenha(){
		try{ 
            servidorSenha = new ServidorSenhaImple(); 
            Naming.rebind("RMI://127.0.0.1:1020/SenhaService", servidorSenha); 
        }  
        catch (Exception e)  
        { 
            e.printStackTrace(); 
        } 
	}
	public static void main(String[] args)  { 
        new ServidorSenha(); 
    } 
}
